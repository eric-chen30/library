<!--pages/booklist/booklist.wxml-->
<view class="header">
   <text>我的收藏</text>
</view>

<view class="bookList_container">
  <scroll-view class="scroll" scroll-y="true" style="height:{{clientHeight}}px">
    <view wx:for="{{bookList}}" wx:key="index" wx:for-item="item">
      <van-card  tag="{{item.type}}"  desc="{{item.tag}}" title="{{item.bookName}}" thumb="{{imageURL}}">
        <view slot="footer">
          <van-button size="mini">借阅</van-button>
          <van-button size="mini">取消</van-button>
        </view>
      </van-card>
    </view>
  </scroll-view>
</view>
